.PHONY: all

OBJS := first second second third third third

all: $(OBJS)
# $$@为shell命令中的变量,而\$$@表示字符串$@,$@为Makefile中的自动变量表示该规则的目标
	@echo "\$$@ = $@"
# $$^表示字符串$^,$^为Makefile中的自动变量表示该规则的所有依赖文件(不含重复)
	@echo "$$^ = $^"
# $$^表示字符串$^,$^为Makefile中的自动变量表示该规则的所有依赖文件(含重复)
	@echo "$$+ = $+"
# $$<表示字符串$<,$<为Makefile中的自动变量表示该规则第一个依赖文件名
	@echo "$$< = $<"
# $$%表示字符串$%,$%为Makefile中的自动变量表示当规则的目标文件是一个静态库文件时，代表静态库的一个成员名
	@echo "$$% = $%"
# $$?为shell命令中的变量,而\$$?表示字符串$?,$?为Makefile中的自动变量表示所有比目标文件更新的依赖文件列表，空格分割
	@echo "\$$? = $?"
# $$*为shell命令中的变量,而\$$*表示字符串$*,$*为Makefile中的自动变量表示在模式规则和静态模式规则中，代表“茎”。“茎”是目标模式中“%”所代表的部分
	@echo "\$$* = $*"
# $@、$<、$%、$*这四个自动变量表示文件名,$^、$+、$?这三个自动变量表示文件名列表

$(OBJS):
